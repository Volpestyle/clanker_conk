{
  "name": "clanker_conk",
  "version": "0.1.0",
  "description": "Discord AI user bot persona for community interaction with dashboard and memory",
  "type": "module",
  "main": "src/app.ts",
  "scripts": {
    "prestart": "npm run build:ui",
    "start": "node src/app.ts",
